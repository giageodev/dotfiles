set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set textwidth=80
set colorcolumn=+1
set nohlsearch
set incsearch
set ignorecase
set showmatch
set tabstop=8
set shiftwidth=8
set scrolloff=10
set nowrap
set smartcase
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
